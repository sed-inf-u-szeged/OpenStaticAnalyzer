      <Enabled>false</Enabled>
      <Group>false</Group>
      <OriginalId>google-default-arguments</OriginalId>
      <GroupMember>google</GroupMember>
      <Language lang = "eng">
        <Warning>true</Warning>
        <DisplayName>Default Arguments</DisplayName>
        <HelpText>&lt;p&gt;Checks that default arguments are not given for virtual methods.&lt;/p&gt;&#x0A;&lt;p&gt;See &lt;a href=&quot;https://google.github.io/styleguide/cppguide.html\#Default_Arguments&quot; class=&quot;uri&quot;&gt;https://google.github.io/styleguide/cppguide.html\#Default_Arguments&lt;/a&gt;&lt;/p&gt;&#x0A;&lt;p&gt;(Clang-Tidy original name: google-default-arguments)&lt;/p&gt;&#x0A;</HelpText>
