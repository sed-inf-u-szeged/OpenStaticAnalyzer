  <Metric id = "CT_GGL_RI">
    <Configuration name = "Default">
      <Enabled>false</Enabled>
      <Group>false</Group>
      <OriginalId>google-runtime-int</OriginalId>
      <GroupMember>google</GroupMember>
      <Language lang = "eng">
